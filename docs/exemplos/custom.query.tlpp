Static Function _xSQL(cLinha)

Local nX
Local cRet := ""

if Upper(Alltrim(cLinha)) == "<COLUNAS>"
	For nX := 1 to Len(Self:aSX3)
		cRet += ", "+alltrim(Self:aSX3[nX,2])
	Next nX
	cLinha := SubStr(cRet,2,Len(cRet))
else
	cLinha := &(cLinha)
endif

Return

